
@{
    ViewBag.Title = "ShowAllManagers";
}

<h2>Действующие менеджеры</h2>

<div>
    <table class="table table-bordered table-hover">
        <tr>
            <td><p>Имя</p></td>
        </tr>
        @foreach (var item in ViewBag.Managers)
        {
            <tr>
                <td><p>@item.LastName</p></td>
            </tr>
        }
    </table>
</div>